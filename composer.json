{
    "name": "zayro/back-end",
    "description": "este es un paquete backend manejando RestFull",
    "scripts": {
        "post-install-cmd": [
            "@composer dump-autoload --optimize"
        ]
    },
    "require": {
        "php": ">=5.6",
        "friendsofphp/php-cs-fixer": "^2.0",
        "phpdocumentor/phpdocumentor": "2.*",
        "bafs/testify": "^1.0",
        "psy/psysh": "@stable"
    },
    "authors": [{
        "name": "Marlon Zayro Arias Vargas",
        "email": "zayro8905@gmail.com"
    }],
    "require-dev": {
        "heroku/heroku-buildpack-php": "*",
        "firebase/php-jwt": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "library\\": "library/",
            "app\\": "app/"
        }
    }
}